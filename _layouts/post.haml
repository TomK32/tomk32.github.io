---
layout: default
---
%h1 {{ page.title}}
%p.meta
  .created-at {{ page.date | date_to_long_string }}
  .categories
    {% if page.categories != '' %}
    Categories:
    {% for category in page.categories %}
    {{ category }}
    {% endfor %}
    {% endif %}
  .tags
    {% if page.tags != '' %}
    Tags:
    {% for tags in page.tags %}
    {{ tags }}
    {% endfor %}
    {% endif %}
{{ content }}
